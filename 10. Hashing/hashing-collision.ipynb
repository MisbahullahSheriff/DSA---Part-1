   "source": [
    "def simple_hash(key):\n",
    "    total = 0\n",
    "    for ch in key:\n",
    "        total += ord(ch)\n",
    "    return total"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "70929904",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "bat: 1\n",
      "ball: 1\n",
      "tab: 1\n"
     ]
    }
   ],
   "source": [
    "keys = ['bat', 'ball', 'tab']\n",
    "for key in keys:\n",
    "    hash_value = simple_hash(key)\n",
    "    print(f'{key}: {hash_value % 5}')"
   ]
  }
